{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "openlab-modules/module-navigation",
    "title": "Module TOC",
    "category": "openlab-modules",
    "icon": "menu",
    "description": "This Table of Contents (TOC) is dynamically generated based on the pages belonging to the module.",
    "supports": {
      "anchor": true,
      "color": {
        "text": true,
        "background": true,
        "gradients": true,
        "link": true
      },
      "spacing": {
        "margin": true,
        "padding": true
      },
      "typography": {
        "fontSize": true,
        "lineHeight": true,
        "fontFamily": true,
        "textTransform": true
      },
      "shadow": true
    },
    "attributes": {
      "moduleId": {
        "type": "integer",
        "default": 0
      },
      "showModuleDescription": {
        "type": "boolean",
        "default": false
      }
    },
    "textdomain": "openlab-modules",
    "editorScript": {
      "handle": "openlab-modules-module-navigation-editor-script",
      "path": "index.js",
      "module": true
    },
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
